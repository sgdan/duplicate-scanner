  "devDependencies": {
    "brunch": "2.10.12",
    "bluebird": "3.5.1",
    "concurrently": "3.5.1",
    "elm": "0.18.0",
    "elm-format": "0.6.1-alpha",
    "elm-test": "0.18.9",
    "elm-brunch": "0.10.0",
    "nugget": "2.0.1"
  }
